.compatibility.merge.wimp <- function(wimp1,wimp2){
merge <- nrow(.merge.wimp(wimp1,wimp2))
if(merge == length(wimp1) && merge == length(wimp2)){
result <- "Full Compatible"
}
if(merge != length(wimp1) | merge != length(wimp2)){
result <- "Partial Compatible"
}
if(merge == 0){
result <- "Incompatible"
}
}
